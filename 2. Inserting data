CREATE DATABASE InsertDataBase;
USE InsertDataBase;

CREATE TABLE student_details(
first_name VARCHAR(25),
last_name VARCHAR(25),
roll_no INT,
email VARCHAR(25),
grade VARCHAR(5)
);

SELECT * FROM student_details;

