CREATE DATABASE details; 
USE details;

CREATE TABLE computer(
sl_no INT PRIMARY KEY AUTO_INCREMENT,
brand VARCHAR(20),
ram INT,
processor VARCHAR(20),
price INT
);

SELECT * FROM computer;

INSERT INTO computer(brand,ram,processor,price) VALUES('Apple',32,'i9',85000);
